  <div id="searchBox" class="container mb-5">

    <div class="row">
      <div class="col-sm">
        <br>
        <h3 class="formHead">Search Events, {{username}}</h3>
      </div>
    </div>

    <hr>

    <div class="row">
      <div class="col-sm">
        <br>
        <h5 class="formHead">Search by:</h5>
      </div>
    </div>

    <div id="searchContainer" class="row">
      <div class="col-sm">
        <div class="accordion" id="searchAccordion">
          
          <div class="card">
            <div class="card-header" id="searchOne">
              <h5 class="mb-0">
                <button class="btn btn-link collapsed" type="button" data-toggle="collapse"
                  data-target="#searchEventName" aria-expanded="true" aria-controls="collapseOne">
                  Event Name
                </button>
              </h5>
            </div>

            <div id="searchEventName" class="collapse" aria-labelledby="headingOne" data-parent="#searchAccordion">
              <div class="card-body">
                <div class="input-group">
                  <input type="text" class="form-control" id="searchName">
                  <span class="input-group-btn">
                    <button id="goButton" class="btn btn-outline-success" type="button" id="goBtn">Go!</button>
                  </span>
                </div>
              </div>
            </div>
          </div>
          <div class="card">
            <div class="card-header" id="searchTwo">
              <h5 class="mb-0">
                <button class="btn btn-link collapsed" type="button" data-toggle="collapse"
                  data-target="#searchEventUser" aria-expanded="true" aria-controls="collapseTwo">
                  User Name
                </button>
              </h5>
            </div>

            <div id="searchEventUser" class="collapse" aria-labelledby="headingTwo" data-parent="#searchAccordion">
              <div class="card-body">
                <div class="input-group">
                  <input type="text" class="form-control" id="searchUser">
                  <span class="input-group-btn">
                    <button class="btn btn-outline-success" type="button" id="goBtn">Go!</button>
                  </span>
                </div>
              </div>
            </div>
          </div>
          <div class="card">
            <div class="card-header" id="searchThree">
              <h5 class="mb-0">
                <button class="btn btn-link collapsed" type="button" data-toggle="collapse"
                  data-target="#searchEventCity" aria-expanded="true" aria-controls="collapseThree">
                  City
                </button>
              </h5>
            </div>

            <div id="searchEventCity" class="collapse" aria-labelledby="headingThree" data-parent="#searchAccordion">
              <div class="card-body">
                <div class="input-group">
                  <input type="text" class="form-control" id="searchCity">
                  <span class="input-group-btn">
                    <button class="btn btn-outline-success" type="button" id="goBtn">Go!</button>
                  </span>
                </div>
              </div>
            </div>
          </div>
          <div class="card">
            <div class="card-header" id="searchFour">
              <h5 class="mb-0">
                <button class="btn btn-link collapsed" type="button" data-toggle="collapse"
                  data-target="#searchEventState" aria-expanded="true" aria-controls="collapseFour">
                  State
                </button>
              </h5>
            </div>

            <div id="searchEventState" class="collapse" aria-labelledby="headingFour" data-parent="#searchAccordion">
              <div class="card-body">
                <div class="input-group">
                  <select class="custom-select" id="searchState" aria-label="select state">
                    <option selected>Choose...</option>
                    <option value="1">DE</option>
                    <option value="2">NJ</option>
                    <option value="3">NY</option>
                    <option value="4">PA</option>
                  </select>
                  <div class="input-group-append">
                    <button class="btn btn-outline-success" type="button" id="goBtn">Go!</button>
                  </div>
                </div>
              </div>
            </div>
          </div>

          
        </div>
      </div>
    </div>

    <hr>

    <div class="row">
        <div class="col-sm">
          {{#each events}}
            {{> event-block}}
          {{/each}}
        </div>
    </div>
  </div>